/*****************Forms******************/
.input-group .form-control{
	z-index:0 !important;
}

/*****************Calendar******************/
.calendar{
    float:left; 
    padding:5px;
    margin: 5px;
}
.calendar table{
    border-collapse: collapse;
    
}
.calendar table td,
.calendar table th{
    height: 30px;
    width: 30px;
    margin: 0;
    padding: 0;
    text-align: center;
    vertical-align: middle;
    
}
.calendar table a{
    display: inline-block;
    background-color: #FFF;
    color: #000;
    height: 30px;
    width: 30px;
    padding: 5px;
    margin: 0;
    text-decoration: none;
    text-align: center;
    vertical-align: middle;
    
}

.calendar table a:hover{
    background-color: #449d44;
	color:#fff;
	border-color:#398439;
}
.calendar table td div.today a{
	background-color: #428bca;
	color:#fff;
	border-color:#357ebd;
}
.calendar table td div.today a:hover{
	background-color: #3071a9;
	color:#fff;
	border-color:#285e8e;
}
/*****************Appointments******************/
